{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Register a Trust",
	"type": "object",
	"properties": {
		"matchData": {
			"type": "object",
			"properties": {
				"utr": {
					"type": "string",
					"pattern": "^[0-9]{10}$"
				},
				"name": {
					"type": "string",
					"pattern": "^[A-Za-z0-9 ,.()/&*'-]{1,56}$"
				},
				"postCode": {
					"type": "string",
					"pattern": "^[a-zA-Z]{1,2}[0-9][0-9a-zA-Z]?\\s?[0-9][a-zA-Z]{2}$"
				}
			},
			"required": [
				"utr",
				"name"
			],
			"additionalProperties": false
		},
		"correspondence": {
			"type": "object",
			"properties": {
				"abroadIndicator": {
					"type": "boolean"
				},
				"name": {
					"type": "string",
					"pattern": "^[A-Za-z0-9 ,.()/&*'-]{1,56}$"
				},
				"address": {
					"$ref": "#/definitions/addressType"
				},
				"phoneNumber": {
					"$ref": "#/definitions/phoneString"
				}
			},
			"required": [
				"abroadIndicator",
				"name",
				"address",
				"phoneNumber"
			],
			"additionalProperties": false
		},
		"yearsReturns": {
			"$ref": "#/definitions/yearsReturnType"
		},
		"declaration": {
			"type": "object",
			"properties": {
				"name": {
					"$ref": "#/definitions/nameType"
				},
				"address": {
					"$ref": "#/definitions/addressType"
				}
			},
			"required": [
				"name",
				"address"
			],
			"additionalProperties": false
		},
		"trust": {
					"$ref": "#/definitions/trustType"
		},
		"agentDetails": {
			"$ref": "#/definitions/agentDetailsType"
		}
	},
	"required": [
		"correspondence",
		"declaration",
		"trust"
	],
	"additionalProperties": false,
	"definitions": {
		"trustType": {
					"type": "object",
					"properties": {
						"details": {
							"$ref": "#/definitions/trustDetailsType"
						},
						"entities": {
							"$ref": "#/definitions/trustEntitiesType"
						},
						"assets": {
							"$ref": "#/definitions/assetsType"
						}
					},
					"required": [
						"details",
						"entities",
						"assets"
					],
					"additionalProperties": false
		},
		"assetsType": {
			"type": "object",
			"properties": {
				"monetary": {
					"type": "array",
					"minItems": 1,
					"maxItems": 1,
					"items": {
						"type": "object",
						"properties": {
							"assetMonetaryAmount": {
								"$ref": "#/definitions/monetaryType"
							}
						},
						"required": [
							"assetMonetaryAmount"
						],
						"additionalProperties": false
					}
				},
				"propertyOrLand": {
					"type": "array",
					"minItems": 1,
					"maxItems": 10,
					"items": {
						"$ref": "#/definitions/propertyLandType"
					}
				},
				"shares": {
					"type": "array",
					"minItems": 1,
					"maxItems": 10,
					"items": {
						"$ref": "#/definitions/sharesType"
					}
				},
				"business": {
					"type": "array",
					"minItems": 1,
					"maxItems": 10,
					"items": {
						"$ref": "#/definitions/businessAssetType"
					}
				},
				"partnerShip": {
					"type": "array",
					"minItems": 1,
					"maxItems": 10,
					"items": {
						"$ref": "#/definitions/partnershipType"
					}
				},
				"other": {
					"type": "array",
					"minItems": 1,
					"maxItems": 10,
					"items": {
						"$ref": "#/definitions/otherAssetType"
					}
				}
			},
			"anyOf":[
				{"required" : ["monetary"] },
				{"required" : ["propertyOrLand"] },
				{"required" : ["shares"]},
				{"required" : ["business"]},
				{"required" : ["partnerShip"]},
				{"required" : ["other"]}
			],
			"additionalProperties": false

		},
		"agentDetailsType": {
			"type": "object",
			"properties": {
				"arn": {
					"type": "string",
					"pattern": "^[A-Z](ARN)[0-9]{7}$"
				},
				"agentName": {
					"type": "string",
					"pattern": "^[A-Za-z0-9 ,.()/&'-]{1,56}$"
				},
				"agentAddress": {
					"$ref": "#/definitions/addressType"
				},
				"agentTelephoneNumber": {
					"$ref": "#/definitions/phoneString"
				},
				"clientReference": {
					"type": "string",
					"pattern": "^[A-Za-z0-9 ,.()/&'-]{1,56}$"
				}
			},
			"required": [
				"arn",
				"agentName",
				"agentAddress",
				"agentTelephoneNumber",
				"clientReference"
			],
			"additionalProperties": false
		},
		"trustEntitiesType": {
			"type": "object",
			"properties": {
				"naturalPerson": {
					"type": "array",
					"minItems": 1,
					"maxItems": 25,
					"items": {
						"$ref": "#/definitions/naturalPersonType"
					}
				},
				"beneficiary": {
					"$ref": "#/definitions/beneficiaryType"
				},
				"deceased": {
					"$ref": "#/definitions/willType"
				},
				"leadTrustees": {
					"$ref": "#/definitions/leadTrusteeType"
				},
				"trustees": {
					"type": "array",
					"minItems": 1,
					"maxItems": 25,
					"items": {
						"$ref": "#/definitions/trusteeType"
					}
				},
				"protectors": {
					"type": "object",
					"properties": {
						"protector": {
							"type": "array",
							"minItems": 1,
							"maxItems": 25,
							"items": [
								{
									"type": "object",
									"properties": {
										"name": {
											"$ref": "#/definitions/nameType"
										},
										"dateOfBirth": {
											"$ref": "#/definitions/dateString"
										},
										"identification": {
											"$ref": "#/definitions/identificationType"
										}
									},
									"required": [
										"name"
									],
									"additionalProperties": false
								}
							]
						},
						"protectorCompany": {
							"type": "array",
							"minItems": 1,
							"maxItems": 25,
							"items": [
								{
									"type": "object",
									"properties": {
										"name": {
											"$ref": "#/definitions/orgNameString"
										},
										"identification": {
											"$ref": "#/definitions/identificationOrgType"
										}
									},
									"required": [
										"name"
									],
									"additionalProperties": false
								}
							]
						}
					},
					"additionalProperties": false
				},
				"settlors": {
					"type": "object",
					"properties": {
						"settlor": {
							"type": "array",
							"minItems": 1,
							"maxItems": 25,
							"items": {
								"type": "object",
								"properties": {
									"name": {
										"$ref": "#/definitions/nameType"
									},
									"dateOfBirth": {
										"$ref": "#/definitions/dateString"
									},
									"identification": {
										"$ref": "#/definitions/identificationType"
									}
								},
								"required": [
									"name"
								],
								"additionalProperties": false
							}
						},
						"settlorCompany": {
							"type": "array",
							"minItems": 1,
							"maxItems": 25,
							"items": {
								"type": "object",
								"properties": {
									"name": {
										"$ref": "#/definitions/orgNameString"
									},
									"companyType": {
										"type": "string",
										"enum": [
											"Trading",
											"Investment"
										]
									},
									"companyTime": {
										"type": "boolean"
									},
									"identification": {
										"$ref": "#/definitions/identificationOrgType"
									}
								},
								"required": [
									"name"
								],
								"additionalProperties": false
							}
						}
					},
					"additionalProperties": false
				}
			},
			"required": [
				"beneficiary",
				"leadTrustees"
			],
			"additionalProperties": false
		},
		"beneficiaryType": {
			"type": "object",
			"properties": {
				"individualDetails": {
					"type": "array",
					"minItems": 1,
					"maxItems": 25,
					"items": {
						"$ref": "#/definitions/individualDetailsType"
					}
				},
				"company": {
					"type": "array",
					"minItems": 1,
					"maxItems": 25,
					"items": {
						"$ref": "#/definitions/companyType"
					}
				},
				"trust": {
					"type": "array",
					"minItems": 1,
					"maxItems": 25,
					"items": {
						"$ref": "#/definitions/beneficiaryTrustType"
					}
				},
				"charity": {
					"type": "array",
					"minItems": 1,
					"maxItems": 25,
					"items": {
						"$ref": "#/definitions/charityType"
					}
				},
				"unidentified": {
					"type": "array",
					"minItems": 1,
					"maxItems": 25,
					"items": {
						"$ref": "#/definitions/unidentifiedType"
					}
				},
				"large": {
					"type": "array",
					"minItems": 1,
					"maxItems": 25,
					"items": {
						"$ref": "#/definitions/largeType"
					}
				},
				"other": {
					"type": "array",
					"minItems": 1,
					"maxItems": 25,
					"items": {
						"$ref": "#/definitions/otherType"
					}
				}
			},
			"anyOf":[
				{"required" : ["individualDetails"]},
				{"required" : ["company"]},
				{"required" : ["trust"]},
				{"required" : ["charity"]},
				{"required" : ["unidentified"]},
				{"required" : ["large"]},
				{"required" : ["other"]}
			],
			"additionalProperties": false
		},
		"individualDetailsType": {
			"type": "object",
			"properties": {
				"name": {
					"$ref": "#/definitions/nameType"
				},
				"dateOfBirth": {
					"$ref": "#/definitions/dateString"
				},
				"vulnerableBeneficiary": {
					"type": "boolean"
				},
				"beneficiaryType": {
					"type": "string",
					"enum": [
						"Director",
						"Employee",
						"NA"
					]
				},
				"beneficiaryDiscretion": {
					"type": "boolean"
				},
				"beneficiaryShareOfIncome": {
					"$ref": "#/definitions/shareofIncomeString"
				},
				"identification": {
					"$ref": "#/definitions/identificationType"
				}
			},
			"required": [
				"name",
				"vulnerableBeneficiary"
			],
			"additionalProperties": false
		},
		"leadTrusteeOrgType": {
			"type": "object",
			"properties": {
				"name": {
					"$ref": "#/definitions/orgName56String"
				},
				"phoneNumber": {
					"$ref": "#/definitions/phoneString"
				},
				"email": {
					"type": "string",
					"format": "email"
				},
				"identification": {
					"$ref": "#/definitions/identificationOrgType"
				}
			},
			"required": [
				"name",
				"phoneNumber",
				"identification"
			],
			"additionalProperties": false
		},
		"trusteeType": {
			"type": "object",
			"properties": {
				"trusteeInd": {
					"$ref": "#/definitions/trusteeIndType"
				},
				"trusteeOrg": {
					"$ref": "#/definitions/trusteeOrgType"
				}
			},
			"additionalProperties": false
		},
		"trusteeOrgType": {
			"type": "object",
			"properties": {
				"name": {
					"$ref": "#/definitions/orgName56String"
				},
				"phoneNumber": {
					"$ref": "#/definitions/phoneString"
				},
				"email": {
					"type": "string",
					"format": "email"
				},
				"identification": {
					"$ref": "#/definitions/identificationOrgType"
				}
			},
			"required": [
				"name"
			],
			"additionalProperties": false
		},
		"trusteeIndType": {
			"type": "object",
			"properties": {
				"name": {
					"$ref": "#/definitions/nameType"
				},
				"dateOfBirth": {
					"$ref": "#/definitions/dateString"
				},
				"identification": {
					"$ref": "#/definitions/identificationType"
				},
				"phoneNumber": {
					"$ref": "#/definitions/phoneString"
				}
			},
			"required": [
				"name"
			],
			"additionalProperties": false
		},
		"leadTrusteeType": {
			"type": "object",
			"oneOf": [
				{
					"type": "object",
					"properties": {
						"leadTrusteeInd": {
							"$ref": "#/definitions/leadTrusteeIndType"
						}
					},
					"required": [
						"leadTrusteeInd"
					],
					"additionalProperties": false
				},
				{
					"type": "object",
					"properties": {
						"leadTrusteeOrg": {
							"$ref": "#/definitions/leadTrusteeOrgType"
						}
					},
					"required": [
						"leadTrusteeOrg"
					],
					"additionalProperties": false
				}
			]
		},
		"leadTrusteeIndType": {
			"type": "object",
			"properties": {
				"name": {
					"$ref": "#/definitions/nameType"
				},
				"dateOfBirth": {
					"$ref": "#/definitions/dateString"
				},
				"identification": {
					"$ref": "#/definitions/identificationTypeNPLT"
				},
				"phoneNumber": {
					"$ref": "#/definitions/phoneString"
				},
				"email": {
					"type": "string",
					"format": "email"
				}
			},
			"required": [
				"name",
				"dateOfBirth",
				"identification",
				"phoneNumber"
			],
			"additionalProperties": false
		},
		"beneficiaryTrustType": {
			"type": "object",
			"properties": {
				"organisationName": {
					"$ref": "#/definitions/orgNameString"
				},
				"beneficiaryDiscretion": {
					"type": "boolean"
				},
				"beneficiaryShareOfIncome": {
					"$ref": "#/definitions/shareofIncomeString"
				},
				"identification": {
					"$ref": "#/definitions/identificationOrgType"
				}
			},
			"required": [
				"organisationName"
			],
			"additionalProperties": false
		},
		"charityType": {
			"type": "object",
			"properties": {
				"organisationName": {
					"$ref": "#/definitions/orgNameString"
				},
				"beneficiaryDiscretion": {
					"type": "boolean"
				},
				"beneficiaryShareOfIncome": {
					"$ref": "#/definitions/shareofIncomeString"
				},
				"identification": {
					"$ref": "#/definitions/identificationOrgType"
				}
			},
			"required": [
				"organisationName"
			],
			"additionalProperties": false
		},
		"unidentifiedType": {
			"type": "object",
			"properties": {
				"description": {
					"type": "string",
					 "pattern": "^[0-9a-zA-Z{\u00C0-\u02FF\u2019}\\- \\u005C&`'^]{1,56}$"
				},
				"beneficiaryDiscretion": {
					"type": "boolean"
				},
				"beneficiaryShareOfIncome": {
					"$ref": "#/definitions/shareofIncomeString"
				}
			},
			"required": [
				"description"
			],
			"additionalProperties": false
		},
		"largeType": {
			"type": "object",
			"properties": {
				"organisationName": {
					"$ref": "#/definitions/orgNameString"
				},
				"description": {
					"type": "string",
					"pattern": "^[0-9a-zA-Z{\u00C0-\u02FF\u2019}\\- \\u005C&`'^]{1,70}$"
				},
				"description1": {
					"type": "string",
					"pattern": "^[0-9a-zA-Z{\u00C0-\u02FF\u2019}\\- \\u005C&`'^]{1,70}$"
				},
				"description2": {
					"type": "string",
					"pattern": "^[0-9a-zA-Z{\u00C0-\u02FF\u2019}\\- \\u005C&`'^]{1,70}$"
				},
				"description3": {
					"type": "string",
					"pattern": "^[0-9a-zA-Z{\u00C0-\u02FF\u2019}\\- \\u005C&`'^]{1,70}$"
				},
				"description4": {
					"type": "string",
					"pattern": "^[0-9a-zA-Z{\u00C0-\u02FF\u2019}\\- \\u005C&`'^]{1,70}$"
				},
				"numberOfBeneficiary": {
					"type": "string",
					"pattern": "^[0-9a-zA-Z]{1,4}$"
				},
				"identification": {
					"$ref": "#/definitions/identificationOrgType"
				},
				"beneficiaryDiscretion": {
					"type": "boolean"
				},
				"beneficiaryShareOfIncome": {
					"$ref": "#/definitions/shareofIncomeString"
				}
			},
			"required": [
				"organisationName",
				"numberOfBeneficiary",
				"description"
			],
			"additionalProperties": false
		},
		"otherType": {
			"type": "object",
			"properties": {
				"description": {
					"type": "string",
					 "pattern": "^[0-9a-zA-Z{\u00C0-\u02FF\u2019}\\- \\u005C&`'^]{1,70}$"
				},
				"address": {
					"$ref": "#/definitions/addressType"
				},
				"beneficiaryDiscretion": {
					"type": "boolean"
				},
				"beneficiaryShareOfIncome": {
					"$ref": "#/definitions/shareofIncomeString"
				}
			},
			"required": [
				"description"
			],
			"additionalProperties": false
		},
		"companyType": {
			"type": "object",
			"properties": {
				"organisationName": {
					"$ref": "#/definitions/orgNameString"
				},
				"beneficiaryDiscretion": {
					"type": "boolean"
				},
				"beneficiaryShareOfIncome": {
					"$ref": "#/definitions/shareofIncomeString"
				},
				"identification": {
					"$ref": "#/definitions/identificationOrgType"
				}
			},
			"required": [
				"organisationName"
			],
			"additionalProperties": false
		},
		"naturalPersonType": {
			"type": "object",
			"properties": {
				"name": {
					"$ref": "#/definitions/nameType"
				},
				"dateOfBirth": {
					"$ref": "#/definitions/dateString"
				},
				"identification": {
					"$ref": "#/definitions/identificationType"
				}
			},
			"required": [
				"name"
			],
			"additionalProperties": false
		},
		"identificationType": {
			"type": "object",
			"oneOf": [
				{
					"type": "object",
					"properties": {
						"nino": {
							"$ref": "#/definitions/ninoType"
						}
					},
					"required": [
						"nino"
					],
					"additionalProperties": false
				},
				{
					"type": "object",
					"properties": {
						"passport": {
							"$ref": "#/definitions/passportType"
						},
						"address": {
							"$ref": "#/definitions/addressType"
						}
					},
					"additionalProperties": false
				}
			]
		},
		"identificationTypeNPLT": {
			"type": "object",
			"oneOf": [{
					"type": "object",
					"properties": {
						"nino": {
							"$ref": "#/definitions/ninoType"
						}
					},
					"required": [
						"nino"
					],
					"additionalProperties": false
				}, {
					"type": "object",
					"properties": {
						"passport": {
							"$ref": "#/definitions/passportType"
						},
						"address": {
							"$ref": "#/definitions/addressType"
						}
					},
					"required": [
						"passport",
						"address"
					],
					"additionalProperties": false
				}
			]
		},
		"identificationOrgType": {
			"type": "object",
			"oneOf": [
				{
					"type": "object",
					"properties": {
						"utr": {
							"$ref": "#/definitions/utrString"
						}
					},
					"required": [
						"utr"
					],
					"additionalProperties": false
				},
				{
					"type": "object",
					"properties": {
						"address": {
							"$ref": "#/definitions/addressType"
						}
					},
					"required": [
						"address"
					],
					"additionalProperties": false
				}
			]
		},
		"shareofIncomeString": {
			"type": "string",
			"pattern": "^([0-9]|[1-9][0-9]|100)$"
		},
		"trustDetailsType": {
			"type": "object",
			"properties": {
				"startDate": {
					"$ref": "#/definitions/dateString"
				},
				"lawCountry": {
					"$ref": "#/definitions/nonUKCountryType"
				},
				"administrationCountry": {
					"$ref": "#/definitions/countryType"
				},
				"residentialStatus": {
					"type": "object",
					"oneOf": [
						{
							"type": "object",
							"properties": {
								"uk": {
									"type": "object",
									"properties": {
