{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Register a Trust or Estate",
	"type": "object",
	"properties": {
		"matchData": {
			"type": "object",
			"properties": {
				"utr": {
					"type": "string",
					"pattern": "^[0-9]{10}$"
				},
				"name": {
					"type": "string",
					"pattern": "^[A-Za-z0-9 ,.()/&*'-]{1,56}$"
				},
				"postCode": {
					"type": "string",
					"pattern": "^[a-zA-Z0-9 ]{1,10}$"
				}
			},
			"required": [
				"utr",
				"name"
			],
			"additionalProperties": false
		},
		"correspondence": {
			"type": "object",
			"properties": {
				"abroadIndicator": {
					"type": "boolean"
				},
				"name": {
					"type": "string",
					"pattern": "^[A-Za-z0-9 ,.()/&*'-]{1,56}$"
				},
				"address": {
					"$ref": "#/definitions/addressType"
				},
				"phoneNumber": {
					"$ref": "#/definitions/phoneString"
				}
			},
			"required": [
				"abroadIndicator",
				"name",
				"address",
				"phoneNumber"
			],
			"additionalProperties": false
		},
		"yearsReturns": {
			"$ref": "#/definitions/yearsReturnType"
		},
		"declaration": {
			"type": "object",
			"properties": {
				"name": {
					"$ref": "#/definitions/nameType"
				},
				"address": {
					"$ref": "#/definitions/addressType"
				}
			},
			"required": [
				"name",
				"address"
			],
			"additionalProperties": false
		},
		"details": {
			"$ref": "#/definitions/estateType"
		},
		"agentDetails": {
			"$ref": "#/definitions/agentDetailsType"
		}
	},
	"required": [
		"correspondence",
		"declaration",
		"details"
	],
	"additionalProperties": false,
	"definitions": {

		"agentDetailsType": {
			"type": "object",
			"properties": {
				"arn": {
					"type": "string",
					"pattern": "^[A-Z](ARN)[0-9]{7}$"
				},
				"agentName": {
					"type": "string",
					"pattern": "^[A-Za-z0-9 ,.()/&'-]{1,56}$"
				},
				"agentAddress": {
					"$ref": "#/definitions/addressType"
				},
				"agentTelephoneNumber": {
					"$ref": "#/definitions/phoneString"
				},
				"clientReference": {
					"type": "string",
					"pattern": "^[0-9a-zA-Z{\u00C0-\u02FF\u2019}\\- \\u005C&`'^]{1,56}$"
				}
			},
			"required": [
				"arn",
				"agentName",
				"agentAddress",
				"agentTelephoneNumber",
				"clientReference"
			],
			"additionalProperties": false
		},
		"identificationType": {
			"type": "object",
			"oneOf": [
				{
					"type": "object",
					"properties": {
						"nino": {
							"$ref": "#/definitions/ninoType"
						}
					},
					"required": [
						"nino"
					],
					"additionalProperties": false
				},
				{
					"type": "object",
					"properties": {
						"passport": {
							"$ref": "#/definitions/passportType"
						},
						"address": {
							"$ref": "#/definitions/addressType"
						}
					},
					"additionalProperties": false
				}
			]
		},
		"identificationOrgType": {
			"type": "object",
			"oneOf": [
				{
					"type": "object",
					"properties": {
						"utr": {
							"$ref": "#/definitions/utrString"
						}
					},
					"required": [
						"utr"
					],
					"additionalProperties": false
				},
				{
					"type": "object",
					"properties": {
						"address": {
							"$ref": "#/definitions/addressType"
						}
					},
					"required": [
						"address"
					],
					"additionalProperties": false
				}
			]
		},
		"estateType": {
			"type": "object",
			"properties": {
				"estate": {
					"type": "object",
					"properties": {
						"entities": {
							"$ref": "#/definitions/entitiesType"
						},
						"administrationEndDate": {
							"$ref": "#/definitions/dateString"
						},
						"periodTaxDues": {
							"description": "01-Total Income Tax and Capital Gains due for the administration period more than £10,000, 02-Date of Death before 05 April 2016 and the sale of estates assets by administrators is £250,000 in any  one tax year, 03-Date of Death After 05 April 2016 and the sale of estates assets by administrators is £500.00 in any one tax year, 04-Estate worth more than 2.5 million at the death of death",
							"type": "string",
							"enum": [
								"01",
								"02",
								"03",
								"04"
							]
						}
					},
					"required": [
						"entities",
						"periodTaxDues"
					],
					"additionalProperties": false
				}
			},
			"required": [
				"estate"
			],
			"additionalProperties": false
		},
		"monetaryType": {
			"description": "Integer up to 12 digits max; 1500 represents 1500 pounds",
			"type": "integer",
			"minimum": 0,
			"maximum": 999999999999
		},

		"yearReturnType": {
			"type": "object",
			"properties": {
				"taxReturnYear": {
					"type": "string",
					"pattern": "^[0-9]{2}$"
				},
				"taxConsequence": {
					"type": "boolean"
				}
			},
			"required": [
				"taxReturnYear",
				"taxConsequence"
			],
			"additionalProperties": false
		},
		"yearsReturnType": {
			"type": "object",
			"properties": {
				"taxReturnsNoDues": {
					"type": "boolean"
				},
				"returns": {
					"type": "array",
					"minItems": 1,
					"maxItems": 4,
					"items": {
						"$ref": "#/definitions/yearReturnType"
					}
				}
			},
			"additionalProperties": false
		},
		"entitiesType": {
			"type": "object",
			"properties": {
				"deceased": {
					"$ref": "#/definitions/willTypeEstate"
				},
				"personalRepresentative": {
					"type": "object",
					"oneOf": [
						{
							"type": "object",
							"$ref": "#/definitions/estatePerRepInd"
						},
						{
							"type": "object",
							"$ref": "#/definitions/estatePerRepOrg"
						}
					]
				}
			},
			"required": [
				"deceased",
				"personalRepresentative"
			],
			"additionalProperties": false
		},
		"passportType": {
			"type": "object",
			"properties": {
				"number": {
					"type": "string",
					"pattern": "^[A-Za-z0-9]{1,30}$"
				},
				"expirationDate": {
					"$ref": "#/definitions/dateString"
				},
				"countryOfIssue": {
					"$ref": "#/definitions/countryType"
				}
			},
			"required": [
				"number",
				"expirationDate",
				"countryOfIssue"
			],
			"additionalProperties": false
		},
		"phoneString": {
			"type": "string",
			"pattern": "^\\+[0-9 ]{1,18}$|^[0-9 ]{1,19}$"
		},
		"willTypeEstate": {
			"type": "object",
			"properties": {
				"name": {
					"$ref": "#/definitions/nameType"
				},
				"dateOfBirth": {
					"$ref": "#/definitions/dateString"
				},
				"dateOfDeath": {
					"$ref": "#/definitions/dateString"
				},
				"identification": {
					"type": "object",
					"oneOf": [
						{
							"type": "object",
							"properties": {
								"nino": {
									"$ref": "#/definitions/ninoType"
								}
							},
							"required": [
								"nino"
							],
							"additionalProperties": false
