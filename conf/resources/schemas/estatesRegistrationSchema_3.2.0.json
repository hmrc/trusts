						{
							"type": "object",
							"properties": {
								"address": {
									"$ref": "#/definitions/addressType"
								}
							},
							"required": [
								"address"
							],
							"additionalProperties": false
						}
					]
				}
			},
			"required": [
				"name",
				"dateOfDeath"
			],
			"additionalProperties": false
		},
